</head>
<body>
    <div class="left-section">
        <div class="sidebar">
            <img src="/img/CORTADO_logo_fundo_grande-removebg-preview 1.png" class="img-fluid" alt="">
            <div class="item-inicio">
                <i class="ri-home-line"></i>
                <h3>Inicio</h3>
            </div>
            <div class="item dashboard">
                <i class="ri-apps-line"></i>
                <h3>Dashboard</h3>
            </div>
            <div class="item">
                <i class="ri-folder-line"></i>
                <h3>Your Files</h3>
            </div>
            <div class="item" id="historico-consultas">
                <i class="ri-health-book-line"></i>
                <h3>Histórico de Consultas</h3>
            </div>
            <div class="item">
                <i class="ri-folder-lock-line"></i>
                <h3>Protected</h3>
            </div>
            <div class="item">
                <i class="ri-delete-bin-5-line"></i>
                <h3>Trash</h3>
            </div>
            <div class="item">
                <i class="ri-settings-3-line"></i>
                <h3>Settings</h3>
            </div>
        </div>

        <div class="item sign-out">
            <button id="logout"><i class="ri-logout-box-r-line"></i></button>
            
            <h3>Sign Out</h3>
        </div>
    </div>

    <div class="main">
        <div class="header">
            <div class="search">
                <button><i class="ri-search-2-line"></i></button>
                <input type="text" placeholder="Search">
                <i class="ri-equalizer-line"></i>
            </div>
            <div class="icon-btns">
                <i class="ri-notification-line"></i>
                <i class="ri-message-3-line"></i>
                <i class="ri-menu-line" id="menu-btn"></i>
            </div>
        </div>

        <h3 class="separator">
            Agendamento
        </h3>
        
        <div id="confirmationModal" class="modal">
            <div class="modal-content">
                <span class="close">&times;</span>
                <h2>Confirmação</h2>
                <p>Você tem certeza que deseja continuar?</p>
                <button id="confirmButton" class="confirmarConsulta">Confirmar</button>
                <button id="cancelButton">Cancelar</button>
            </div>
          </div>

        <div class="quick-access">
            <div class="item " id ="agendar-consulta">
                <i class="ri-empathize-line"></i>
                <h5>Agendar Consulta</h5>
            </div>
            <div class="item " id="criar-agenda">
                <i class="ri-calendar-2-line"></i>
                <h5>Criar Agenda</h5>
            </div>
            <div class="item " id="prontuario">
                <i class="ri-todo-line"></i>
                <h5>Prontuários</h5>
            </div>
            <div class="item " id="verificarAgenda">
                <i class="ri-calendar-2-line"></i>
                <h5>Criar Agenda</h5>
            </div>
        </div>

        <h3 class="separator">
            Consultas
        </h3>

        <table class="tableHorario">
            <thead>
                <th>Data</th>
                <th>Dia da Semana</th>
                <th>Hora</th>
                <th>status</th>
            </thead>
            <tbody>
                
            </tbody>
        </table>

    </div>

    <div class="right-section">
        <div class="profile" id="conta">
            <div class="info">
                <i style="font-size: 25px;" class="ri-account-circle-line"></i>
                <div class="account">
                    <h5></h5>
                    <p></p>
                </div>
            </div>
            
        </div>

        <!-- <div class="widgets">
            <div class="disk">
                <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="25" aria-valuemin="0"
                        aria-valuemax="100"></div>
                </div>
                <div class="info">
                    <h5><span>750</span> GB Free<span> / 1</span> TB</h5>
                    <p>Available Storage</p>
                </div>
            </div>

            <div class="bottom">
                <button>New Upload <i class="ri-add-line"></i></button>

                <div class="premium">
                    <div class="title">
                        <i class="ri-folder-add-line"></i>
                        <h5>Buy Premium</h5>
                    </div>
                    <p>Upgrade space now and get a 18% discount.</p>
                </div>
            </div>
        </div> -->
    </div>
    
    
<script src="https://code.jquery.com/jquery-3.7.1.js" integrity="sha256-eKhayi8LEQwp4NKxN+CfCh+3qOVUtJn3QNZ0TciWLP4=" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
<script type="module" src="/js/usuario/principal/ajaxPrinc.js"></script>
<script type="module" src="/js/usuario/principal/funcaoPrinc.js"></script>  
<script src="/js/usuario/principal/formatPrinc.js"></script> 
