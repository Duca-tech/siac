<body>

  <div class="left-section">
    <div class="sidebar">
        <img src="/img/CORTADO_logo_fundo_grande-removebg-preview 1.png" class="img-fluid" alt="">
        <div class="item inicio">
            <i class="ri-home-line"></i>
            <h3>Inicio</h3>
        </div>
        <div class="item">
            <i class="ri-folder-line"></i>
            <h3>Your Files</h3>
        </div>
        <div class="item">
            <i class="ri-hard-drive-3-line"></i>
            <h3>Your Disks</h3>
        </div>
        <div class="item">
            <i class="ri-folder-lock-line"></i>
            <h3>Protected</h3>
        </div>
        <div class="item">
            <i class="ri-delete-bin-5-line"></i>
            <h3>Trash</h3>
        </div>
        <div class="item">
            <i class="ri-settings-3-line"></i>
            <h3>Settings</h3>
        </div>
    </div>

    <div class="item sign-out">
        <button id="logout"><i class="ri-logout-box-r-line"></i></button>
        
        <h3>Sign Out</h3>
    </div>
</div>

<div class="main">
    <div class="header">
        <div class="search">
            <button><i class="ri-search-2-line"></i></button>
            <input type="text" placeholder="Search">
            <i class="ri-equalizer-line"></i>
        </div>
        <div class="icon-btns">
            <i class="ri-notification-line"></i>
            <i class="ri-message-3-line"></i>
            <i class="ri-menu-line" id="menu-btn"></i>
        </div>
    </div>

    <h5 class="separator">Criar Agenda</h5>

    <div class="containerSelect">

    </div>

    <div>
      
      <div>
          <div  id="containerAgenda">

              <label for="date">Data da agenda</label>
              <input type="date" id="date"> <br><br>
      
              <div id="containerDiaSemana">
                  <label for="diaSemana">Dia da semana</label>
                  <input type="text" id="diaSemana" required disabled>
              </div>
              <label for="horaIni">Primeiro Horario de Atendimento</label>
              <input type="time" id="horaIni" required> <br><br>
      
              <label for="horaFin">Ultimo Horario de Atendimento</label>
              <input type="time" id="horaFin" required> <br><br>
      
              <button class="btn btn-success" id="gerarAgenda">Gerar Agenda</button>
              
          </div>
          
          <div class="container " id="ContainerAgendaDados">
             
              <table class="borda" id="tabelaAgenda">
                  
              </table>
      
          </div>
      </div>

      
  </div>

</div>

<div class="right-section">
    <div class="profile" id="conta">
        <div class="info">
            <i style="font-size: 25px;" class="ri-account-circle-line"></i>
            <div class="account">
                <h5></h5>
                <p></p>
            </div>
        </div>
        
    </div>

