    <script type="module" src="/js/usuario/redefinirSenha/ajax.js"></script>
    <script type="module" src="/js/usuario/redefinirSenha/funcao.js"></script>
    <script type="module" src="/js/usuario/esqueciSenha/ajax.js"></script>
</body>
</html>
