    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/esqueciSenha/redefinirSenha.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
    <title>Pagina Esqueci Senha</title>
</head>
<body>
    <main>
        <div class="containerForm">
            <form action="">
                <div>
                    <label>Insira Nova senha:</label><br>
                    <input type="password" id="primeiraSenha">
                </div>
                <div>
                    <label>Confirme sua Senha</label><br>
                    <input type="password" id="segundaSenha">
                </div>
                <div>
                    <button type="button" class="btn btn-success" id="enviarNovaSenha">Enviar</button>
                </div>
            </form>
        </div>
    </main>
    
    <script src="https://code.jquery.com/jquery-3.7.1.js" integrity="sha256-eKhayi8LEQwp4NKxN+CfCh+3qOVUtJn3QNZ0TciWLP4=" crossorigin="anonymous"></script>
